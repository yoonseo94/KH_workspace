package kh.java.reflection;

public class Main {

	public static void main(String[] args) {
		Main main = new Main();
		main.test1();
	}

	/**
	 * Reflection API
	 * - 클래스객체의 정보를 가지고 객체를 제어하는 기술
	 * - 생성자호출 | 메소드호출 | 필드값 제어
	 */
	private void test1() {
		Sample sample1 = new Sample();
		System.out.println(sample1);
	}

}
