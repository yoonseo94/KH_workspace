<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="student">
  <select id="selectOne" parameterType="_int" resultType="student">
  	select
  		*
  	from 
  		student
  	where
  		deleted_at is null
  	  and 
  	  	no = #{no}
  </select>
</mapper>